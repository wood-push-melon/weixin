/**app.wxss**/
.main-page{
  height: 100%;
  background: #f5f5f5;
  padding-bottom: 200rpx;
}
.detail-navigator{
  color: #78D1E9;
}
.vertical-space{
  padding-top: 20rpx;
} 
.article-fulltext-page{
  background: white;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
}
.article-fulltext-title{
  color: black;
  font-size: 40rpx;
  font-weight: bold;
  text-align: center;
  margin-bottom: 30rpx;
}
.article-fulltext-text{
  color: black;
  text-indent: 56rpx;
  margin-bottom: 10rpx;
}
.article-fulltext-subtitle{
  color: black;
  font-weight: bold;
  text-indent: 56rpx;
  margin-top: 20rpx; 
  margin-bottom: 10rpx;
}
.article-fulltext-image{
  width: 90%;
  margin: 10rpx 5% 20rpx 5%;
}

input{font-size: 32rpx; color: grey; font-family: "微软雅黑";}
button{font-size: 36rpx; color: grey; font-family: "微软雅黑";}
text{font-size: 28rpx; color: grey; font-family: "微软雅黑"}
